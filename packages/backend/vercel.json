{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "devCommand": "yarn dev",
  "buildCommand": "yarn build",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
